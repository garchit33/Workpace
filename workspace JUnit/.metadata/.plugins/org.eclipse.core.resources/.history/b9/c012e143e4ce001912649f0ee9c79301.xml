<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>ServletConfigExample</display-name>
  <servlet>
    <servlet-name>TestContext</servlet-name>
    <servlet-class>testcontext.TestContext</servlet-class>
    <init-param>
      <param-name>username</param-name>
      <param-value>archit</param-value>
    </init-param>
    <init-param>
      <param-name>email</param-name>
      <param-value>garchit33@gmail.com</param-value>
    </init-param>
  </servlet>
  
  <servlet>
  <servlet-name>ConfigServlet</servlet-name>
  <servlet-class>testcontext.ConfigServlet</servlet-class>
  </servlet>
  <context-param>
  <param-name>username</param-name>
  <param-value>archit</param-value>
  </context-param>
  <context-param>
  <param-name>email</param-name>
  <param-value>garchit33@gmail.com</param-value>
  </context-param>
  <context-param>
  <param-name>contact</param-name>
  <param-value>7899466445</param-value>
  </context-param>
  <servlet-mapping>
    <servlet-name>TestContext</servlet-name>
    <url-pattern>/TestContext</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ConfigServlet</servlet-name>
    <url-pattern>/ConfigServlet</url-pattern>
  </servlet-mapping>
</web-app>